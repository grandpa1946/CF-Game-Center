{
  "name": "cloudforce-game-center",
  "version": "1.0.0",
  "description": "Cloudforce game center",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "build": "electron-forge make",
    "make": "electron-forge make --platform=win32",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder --windows  --publish always"
  },
  "build": {
    "appId": "com.cloudforce.gamecenter",
    "asar": true,
    "win": {
      "icon": "./assets/icons/cloudforce.ico"
    }
  },
  "author": "printedwaste",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "@sentry/electron": "^4.10.0",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2"
  }
}
